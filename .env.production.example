# .env.production.example
# Copy to .env.production and fill with real values

# Environment
NODE_ENV=production

# Security
JWT_SECRET=your-super-secure-jwt-secret-change-in-production

# Database
DB_USER=kamina_prod
DB_PASSWORD=your-secure-database-password

# Blockchain
BLOCKCHAIN_RPC_URL=https://mainnet.infura.io/v3/your-infura-key
BLOCKCHAIN_PRIVATE_KEY=your-secure-private-key

# AI Service
AI_API_KEY=your-kaminallm-api-key
AI_BASE_URL=https://ai.kamina-os.com

# Redis
REDIS_PASSWORD=your-redis-password

# Domain
DOMAIN=kamina-os.com
